{
  "manifest_version": 2,

  "name": "Autobookmark",
  "description": "Automatically bookmark a page after a set amount of time!",
  "version": "1.0",
  "author": "23nicolaso@gmail.com",

  "permissions": [
    "tabs",
    "bookmarks"
  ],
  "icons": {
    "16": "clock16.png",
    "48": "clock48.png",
    "128": "clock128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "browser-action-19.png",
      "38": "browser-action-38.png"
    }
  },
  "background": {
    "scripts": [
      "common.js",
      "background.js"
      ],
    "persistent": true
  },
  "options_page": "options.html"
}
